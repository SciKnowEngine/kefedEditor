		private var nCitations:int = 0;
		private var nthCitation:int = 0;
		private function updateCitationsHandler():void {
			updateCitations.enabled = false;
			nthCitation = 0;
			nCitations = templateList.length;
			doNextTemplateCitation();
		}
				
		private var xCitations:int = 0;
		private var xthCitation:int = 0;
		private function updateXCitationsHandler():void {
			updateXCitations.enabled = false;
			xthCitation = 0;
			xCitations = experimentList.length;
			doNextExperimentCitation();
		}
		
		private function updateTemplateCitation(e:ModelStoreEvent):void {
			trace("Update template Citation handler, model=",e.model.modelName);
			CursorManager.removeBusyCursor();
			this.template = e.model;
			if (template.source) {
				var mappedSource:String = citationMap[template.source];
				if (mappedSource) {
					template.citeKey = template.source;
					template.source = mappedSource;
					status.text=nthCitation + ": Mapped " + template.citeKey + " to " + template.source;
					trace("  Mapped " + template.citeKey + " to " + template.source);
					CursorManager.setBusyCursor();
					modelStore.saveModel(template);
				} else {
					trace("  No mapping for " + template.source);
					status.text=nthCitation + ": No mapping for " + template.source;
					doNextTemplateCitation();
				}
			}
		}
		
		private function updateExperimentCitation(e:DataStoreEvent):void {
			trace("Update template Citation handler, model=",e.experiment.modelName);
			CursorManager.removeBusyCursor();
			this.experiment = e.experiment;
			if (experiment.source) {
				var mappedSource:String = citationMap[experiment.source];
				if (mappedSource) {
					experiment.citeKey = experiment.source;
					experiment.source = mappedSource;
					xStatus.text=xthCitation + ": Mapped " + experiment.citeKey + " to " + experiment.source;
					trace("  Mapped " + experiment.citeKey + " to " + experiment.source);
					CursorManager.setBusyCursor();
					dataStore.saveData(experiment);
				} else {
					trace("  No mapping for " + experiment.source);
					xStatus.text=xthCitation + ": No mapping for " + experiment.source;
					doNextExperimentCitation();
				}
			}
		}
		
		private function processNextTemplateCitation(e:ModelStoreEvent):void {
			CursorManager.removeBusyCursor();
			doNextTemplateCitation();
		}
		private function processNextExperimentCitation(e:DataStoreEvent):void {
			CursorManager.removeBusyCursor();
			doNextExperimentCitation();
		}
		
		private function doNextTemplateCitation():void {
			if (nthCitation < nCitations) {
				cursorManager.setBusyCursor();
				modelStore.retrieveModel(templateList.getItemAt(nthCitation).uid);
				nthCitation++;
			} else {
				updateCitations.enabled = true;
				trace("Finished " + nCitations);
				status.text="Done (" + nCitations + ")";
				refresh();				
			}
		}
		private function doNextExperimentCitation():void {
			if (xthCitation < xCitations) {
				cursorManager.setBusyCursor();
				dataStore.retrieveData(experimentList.getItemAt(xthCitation).uid);
				xthCitation++;
			} else {
				updateCitations.enabled = true;
				xStatus.text="Done (" + xCitations + ")";
				trace("Finished " + xCitations);
				refresh();				
			}
		}
		
		private function handleListKefedTemplates(e:ModelStoreEvent):void {
			// trace("Handle List Templates");
			CursorManager.removeBusyCursor();
			this.templateList = e.modelList;
			templateGrid.selectedIndex = -1;
		}
		
		private function handleListKefedExperimentss(e:DataStoreEvent):void {
			CursorManager.removeBusyCursor();
			this.experimentList = e.experimentList;
			experimentGrid.selectedIndex = -1;
		}

    	public function initInventory():void {
    		
    		setupCitationMap();
    		   		
    		modelStore.addEventListener(ModelStoreEvent.LIST, handleListKefedTemplates);
    		modelStore.addEventListener(ModelStoreEvent.RETRIEVE, updateTemplateCitation);
    		modelStore.addEventListener(ModelStoreEvent.SAVE, processNextTemplateCitation);
    		modelStore.addEventListener(FaultEvent.FAULT, FaultEventHandler);

    		dataStore.addEventListener(DataStoreEvent.LIST, handleListKefedExperimentss);
     		dataStore.addEventListener(DataStoreEvent.RETRIEVE, updateExperimentCitation);
    		dataStore.addEventListener(DataStoreEvent.SAVE, processNextExperimentCitation);
    		dataStore.addEventListener(FaultEvent.FAULT, FaultEventHandler);
   		
		}
		
	 public function refresh(e:Event=null):void {
    		CursorManager.setBusyCursor();
			modelStore.listModels();
			CursorManager.setBusyCursor(); // Set again, since each handler pops one.
    		dataStore.listData();		
    	}

			
		]]>
